<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Api Suite" verbose="10" annotations="JDK" thread-count="2" parallel="classes">

    <listeners>
        <listener class-name="com.cocktaildb.testng.listeners.ApiTestListenerAdapter"/>
    </listeners>

    <test name="Api Tests" preserve-order="true">
        <classes>
            <class name="com.cocktaildb.CocktaildbTests"/>
        </classes>
    </test>

</suite>